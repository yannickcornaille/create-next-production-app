  "stylelint.validate": ["css"],
  "editor.formatOnSave": true,
  "jest.jestCommandLine": "npm run test --",
  "jest.runMode": "on-demand",
  "i18n-ally.localesPaths": ["src/locales"],
  "i18n-ally.keystyle": "nested"
}
